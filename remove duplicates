https://practice.geeksforgeeks.org/problems/remove-duplicate-elements-from-sorted-array/1?page=3&company[]=Microsoft&sortBy=submissions

int remove_duplicate(int a[],int n){
        // code here
        int count=1;
        int dupli=a[0];
        int j=1;
        for(int i=1;i<n;i++){
            if(a[i]!=dupli){
                dupli=a[i];
                a[j]=a[i];
                j++;
                count++;
            }
        }
        return count;
    }
  
